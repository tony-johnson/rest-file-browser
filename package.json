{
  "name": "lit-element-starter-js",
  "version": "0.0.0",
  "description": "A simple web component",
  "main": "file-browser.js",
  "module": "file-browser.js",
  "type": "module",
  "scripts": {
    "lint": "lit-analyzer file-browser.js && eslint '**/*.js'",
    "format": "prettier file-browser.js test/* --write",
    "docs": "npm run docs:clean && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/file-browser.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "es-dev-server --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"**/*.js\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch --config es-dev-server.config.cjs",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat file-browser.bundled.js | gzip -9 | wc -c ; rm file-browser.bundled.js",
    "build": "echo \"This is not a TypeScript project, so no need to build.\"",
    "rollup": "rollup -c"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@js-joda/core": "^3.2.0",
    "ace-builds": "^1.4.0",
    "firebase": "^9.0.0-beta.1",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^3.4.8",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^7.0.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^2.1.0",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-analyzer": "^1.2.1",
    "mocha": "^7.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1",
    "web-component-analyzer": "^1.1.6"
  }
}
